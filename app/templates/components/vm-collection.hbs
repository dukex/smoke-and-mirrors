{{#each __content key=keyForId as |item|}}
  {{#vm-item
    defaultHeight=defaultHeight
    tagName=itemTagName
    content=item
    alwaysUseDefaultHeight=alwaysUseDefaultHeight
    keyForId=keyForId
    as |wrapper|
  }}
    {{#if wrapper.contentInserted}}
      {{yield item}}
    {{/if}}
  {{/vm-item}}
{{/each}}